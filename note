- name: Get the GID of the group
  shell: getent group ace-ds-admin-prod | cut -d: -f3
  register: group_gid
  ignore_errors: yes

- name: Display the GID
  debug:
    msg: "The GID of ace-ds-admin-prod is {{ group_gid.stdout }}"

- name: Use the GID in another task
  shell: echo "Group ID is {{ group_gid.stdout }}"
