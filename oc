sudo mount -t nfs4 -o nfsvers=4.1 fs-84647b71.efs.us-east-1.amazonaws.com:/ai-eng /mnt/test


# Define your variables
efs_id="fs-xxxxxxxx"
region="us-east-1"
access_point_id="fsap-087264ee9ed33c076"
mount_path="/ml-ace-efs/ai-eng"

# Create the mount path if it doesn't exist
sudo mkdir -p "$mount_path"

# Mount the EFS access point
sudo mount -t efs -o tls,_netdev,iam,accesspoint=${access_point_id} \
"${efs_id}.efs.${region}.amazonaws.com:/" "$mount_path"
